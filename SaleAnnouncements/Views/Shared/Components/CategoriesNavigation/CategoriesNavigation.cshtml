@model IReadOnlyCollection<SaleAnnouncements.BLL.Dto.CategoryDto>

<cache expires-sliding="@TimeSpan.FromMinutes(60)">
	<div class="dropdown-menu">
		@foreach (var category in Model)
		{
			<a class="dropdown-item" asp-controller="Home" asp-action="Category" asp-route-id="@category.Id">@category.Name</a>
		}
	</div>
</cache>